{
  "name": "Imi Prompt",
  "version": "1.0",
  "manifest_version": 3,
  "description": "My React App as a Chrome Extension",
  "action": {
    "default_title": "Imi Prompt",
    "default_popup": "index.html",
    "default_icon": {
      "16": "logo192.png",
      "32": "logo192.png",
      "48": "logo192.png",
      "128": "logo192.png"
    }
  },
  "permissions": [
    "activeTab",
    "storage",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./static/js/main.f5262714.js"],
      "run_at": "document_end"
    }
  ]
}
